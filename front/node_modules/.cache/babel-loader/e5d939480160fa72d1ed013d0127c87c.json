mg\" style=\"margin-right:20px;\" v-bind:key=\"index\" :src=\"ruleForm.touxiang.split(',')[0]\" width=\"100\" height=\"100\">\r\n                <img v-else class=\"upload-img\" style=\"margin-right:20px;\" v-bind:key=\"index\" v-for=\"(item,index) in ruleForm.touxiang.split(',')\" :src=\"baseUrl+item\" width=\"100\" height=\"100\">\r\n            </el-form-item>\r\n          <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}'  label=\"班级\" prop=\"banji\">\r\n            <el-select v-model=\"ruleForm.banji\" placeholder=\"请选择班级\" :disabled=\" false  ||ro.banji\" >\r\n              <el-option\r\n                  v-for=\"(item,index) in banjiOptions\"\r\n                  :key=\"index\"\r\n                  :label=\"item\"\r\n                  :value=\"item\">\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"width\":\"100%\",\"padding\":\"10px\",\"margin\":\"0 0 10px\",\"background\":\"none\",\"display\":\"inline-block\"}'  label=\"兴趣课程\" prop=\"kechengfenlei\">\r\n            <el-select v-model=\"ruleForm.kechengfenlei\" placeholder=\"请选择兴趣课程\" :disabled=\" false  ||ro.kechengfenlei\" >\r\n              <el-option\r\n                  v-for=\"(item,index) in kechengfenleiOptions\"\r\n                  :key=\"index\"\r\n                  :label=\"item\"\r\n                  :value=\"item\">\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n\r\n      <el-form-item :style='{\"padding\":\"0\",\"margin\":\"0\"}'>\r\n        <el-button :style='{\"border\":\"0\",\"cursor\":\"pointer\",\"padding\":\"0\",\"margin\":\"0 20px 0 0\",\"outline\":\"none\",\"color\":\"rgba(255, 255, 255, 1)\",\"borderRadius\":\"6px\",\"background\":\"#1e3c4f\",\"width\":\"128px\",\"lineHeight\":\"36px\",\"fontSize\":\"14px\",\"height\":\"36px\"}'  type=\"primary\" @click=\"onSubmit\">提交</el-butt